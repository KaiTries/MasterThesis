@prefix td: <https://www.w3.org/2019/wot/td#>.
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix htv: <http://www.w3.org/2011/http#> .
@prefix bspl: <https://purl.org/hmas/bspl/> .

<#artifact>
    bspl:hasRole [ a bspl:Role ;
        bspl:roleName "Seller" ;
        bspl:protocolName "Buy" ;
    ] ;
    td:hasActionAffordance [ a td:ActionAffordance;
    td:name "sendMessage";
    td:hasForm [
        htv:methodName "GET";
        hctl:hasTarget <http://127.0.0.1:8010/>;
        hctl:forContentType "text/plain";
        hctl:hasOperationType td:invokeAction;
    ]
].


