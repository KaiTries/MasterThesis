@base <http://localhost:8080/> .
@prefix hmas: <https://purl.org/hmas/> .
@prefix wotsec: <https://www.w3.org/2019/wot/security#> .
@prefix td: <https://www.w3.org/2019/wot/td#> .
@prefix example: <> .

<workspaces/bazaar/artifacts/rug#artifact> a td:Thing, hmas:Artifact;
  td:title "rug";
  td:hasSecurityConfiguration [ a wotsec:NoSecurityScheme
    ];
  hmas:isContainedIn <workspaces/bazaar#workspace> .

<workspaces/bazaar/artifacts/rug> a hmas:ResourceProfile, example:Rug, gr:ActualProductOrServiceInstance;
  hmas:isProfileOf <workspaces/bazaar/artifacts/rug#artifact> .

<workspaces/bazaar#workspace> a hmas:Workspace .
